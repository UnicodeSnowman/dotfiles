function md() {
    mkdir -p "$@" && cd "$@"
}

function server {
   python -m SimpleHTTPServer $1
}

function geeknote {
    if [ -f /usr/local/Cellar/geeknote/geeknote.py ]; then
        python /usr/local/Cellar/geeknote/geeknote.py "$@"
    fi
}

